{
  "name": "@toznysecure/account-sdk",
  "version": "2.0.14",
  "description": "Javascript SDK for performing Tozny platform account level operations",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -fr dist && tsc",
    "docs": "typedoc",
    "lint": "eslint --fix 'src/'",
    "prettier": "prettier --write src",
    "test": "jest",
    "tp": "./scripts/tozny_platform.sh",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tozny/js-account-sdk.git"
  },
  "keywords": [
    "crypto",
    "tozny",
    "js",
    "javascript",
    "account",
    "administration"
  ],
  "author": {
    "name": "Tozny, LLC",
    "email": "info@tozny.com",
    "url": "https://tozny.com"
  },
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/tozny/js-account-sdk/issues"
  },
  "homepage": "https://github.com/tozny/js-account-sdk#readme",
  "dependencies": {
    "isomorphic-fetch": "^3.0.0",
    "niceware": "^2.0.0"
  },
  "devDependencies": {
    "@toznysecure/eslint-config": "^1.0.0",
    "@toznysecure/prettier-config": "^1.0.0",
    "@toznysecure/sdk": "^1.3.6",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.24",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv": "^8.2.0",
    "eslint": "^8.7.0",
    "eslint-plugin-jest": "^23.8.2",
    "glob-parent": ">=5.1.2",
    "husky": "^4.2.3",
    "jest": "^27.0.6",
    "lint-staged": "^10.0.8",
    "prettier": "^2.5.1",
    "semver-regex": ">=3.1.3",
    "set-value": ">=4.0.1",
    "tmpl": ">=1.0.5",
    "ts-jest": "^27.0.4",
    "typedoc": "^0.22.11",
    "typedoc-plugin-markdown": "^3.11.2",
    "typescript": "^4.5.4",
    "uuid": "^7.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run docs"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.ts": [
      "npm run typecheck",
      "eslint --fix",
      "prettier --write"
    ]
  },
  "prettier": "@toznysecure/prettier-config"
}
